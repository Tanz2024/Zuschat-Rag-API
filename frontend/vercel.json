{
  "version": 2,
  "name": "zus-coffee-chatbot-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "ZUS Coffee AI Assistant",
    "NEXT_PUBLIC_APP_VERSION": "2.2.0"
  },
  "functions": {
    "pages/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "crons": []
}
